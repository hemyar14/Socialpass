#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/Socialpass/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_6c338c3194ccf76c69fc37ee17aa2560/node_modules/ts-jest/node_modules:/workspaces/Socialpass/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_6c338c3194ccf76c69fc37ee17aa2560/node_modules:/workspaces/Socialpass/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/Socialpass/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_6c338c3194ccf76c69fc37ee17aa2560/node_modules/ts-jest/node_modules:/workspaces/Socialpass/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_6c338c3194ccf76c69fc37ee17aa2560/node_modules:/workspaces/Socialpass/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
